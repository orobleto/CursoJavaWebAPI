<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Validacion</servlet-name>
		<display-name>Validacion</display-name>
		<description></description>
		<servlet-class>com.curso.java.servlets.Validacion</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Prueba</servlet-name>
		<display-name>Prueba</display-name>
		<description></description>
		<servlet-class>com.curso.java.servlets.Prueba</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Validacion</servlet-name>
		<url-pattern>/Validacion</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Prueba</servlet-name>
		<url-pattern>/Prueba</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>1</session-timeout>
	</session-config>
</web-app>
